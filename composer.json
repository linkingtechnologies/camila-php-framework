{
    "require": {
		"php": "^8.3",
        "adodb/adodb-php": "dev-master",
		"rospdf/pdf-php": "0.12.67",
		"phpoffice/phpspreadsheet": "4.2.0",
		"tinybutstrong/tinybutstrong": "3.15.2",
		"wizaplace/php-etl": "2.3.0",
		"bordercloud/sparql": "2.1.0",
		"ezyang/htmlpurifier": "4.18.0",
		"lebedevsergey/odt2xhtml": "1.0",
		"phpseclib/phpseclib" : "~3.0",
		"neitanod/forceutf8": "~2.0",
		"psr/http-message": "*",
		"psr/http-factory": "*",
		"psr/http-server-handler": "*",
		"psr/http-server-middleware": "*",
		"nyholm/psr7": "*",
		"nyholm/psr7-server": "*",
		"mpdf/mpdf": "*",
		"phpoffice/phpword": "*",
        "ext-sqlite3": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-exif": "*",
        "ext-pdo_sqlite": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-mysqli": "*",
        "ext-pdo_mysql": "*",
        "ext-xsl": "*",
        "ext-zip": "*"		
	},
	"config": {
        "platform": {
            "php": "8.3"
        },
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true,
		"discard-changes": true
    },
	"archive": {
		"exclude": [
			"*/tests/*",
			"*/docs/*",
			"*/examples/*",
			"*/.github/*",
			"*/.git/*",
			"*/.gitignore",
			"*/.travis.yml",
			"*/phpunit.xml*",
			"*/composer.json",
			"*/composer.lock",
			"*/README*",
			"*/CHANGELOG*"
		]
	}
}